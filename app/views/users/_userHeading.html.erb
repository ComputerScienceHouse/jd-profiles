<div id="image_bar">
    <h3>
    <% if @user["onfloor"] == ["1"] || @user["onfloor"] == [["1"], :single] %>
        Active - on-floor
    <% elsif @user["onfloor"] == ["0"] || @user["onfloor"] == [["0"], :single] %>
        Active - off-floor
    <% else %>
        Alumni
    <% end %>
    <img class="thumbnail" src="/image/<%= uid %>">
</div>
<h3 style="margin-left:10px; margin-top:30px;"><%= cn %></h3>
<br>

